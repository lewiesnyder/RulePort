{
  "name": "ruleport",
  "version": "0.1.0",
  "description": "Unified AI assistant rules syncing utility for directory-based rules",
  "type": "module",
  "main": "dist/cli.js",
  "bin": {
    "ruleport": "./dist/cli.js"
  },
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc",
    "dev": "tsx src/cli.ts",
    "sync": "npm run build && node dist/cli.js",
    "sync:watch": "npm run build && node dist/cli.js --watch",
    "check": "npm run build && node dist/cli.js check",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "cursor",
    "copilot",
    "claude-code",
    "antigravity",
    "ai-rules"
  ],
  "dependencies": {
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@vitest/coverage-v8": "^1.6.1",
    "@vitest/ui": "^1.2.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "author": "",
  "license": "MIT"
}
